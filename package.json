{
  "name": "ore-etl",
  "version": "1.0.0",
  "description": "ETL pipeline for ORE protocol transaction data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test:connection": "node test-connection.js",
    "test:extract": "node scripts/extract-samples.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "realtime": "ts-node src/etl/run-realtime.ts",
    "realtime:dev": "ts-node --watch src/etl/run-realtime.ts",
    "start:realtime": "node dist/etl/run-realtime.js",
    "etl:deploy": "ts-node src/etl/run-deploy.ts",
    "etl:checkpoint": "ts-node src/etl/run-checkpoint.ts",
    "etl:claim-sol": "ts-node src/etl/run-claim-sol.ts",
    "etl:claim-ore": "ts-node src/etl/run-claim-ore.ts",
    "etl:deposit": "ts-node src/etl/run-deposit.ts",
    "etl:withdraw": "ts-node src/etl/run-withdraw.ts",
    "etl:claim-yield": "ts-node src/etl/run-claim-yield.ts",
    "etl:bury": "ts-node src/etl/run-bury.ts",
    "etl:reset": "ts-node src/etl/run-reset.ts",
    "etl:all": "ts-node src/etl/run-all.ts",
    "etl:transformer": "ts-node src/etl/run-transformer.ts",
    "etl:transformer:sample": "ts-node src/etl/run-transformer-sample.ts --limit 100",
    "query": "ts-node src/queries/sample-queries.ts",
    "analytics": "ts-node src/analytics/examples.ts",
    "inspect:dlq": "ts-node inspect-dlq.ts",
    "test:parse": "ts-node test-parse-transaction.ts",
    "analyze:dlq": "ts-node analyze-dlq-signatures.ts",
    "purge:dlq": "ts-node purge-dlq.ts",
    "purge:queue": "ts-node purge-queue.ts",
    "fix:squares": "ts-node fix-squares.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.2",
    "amqplib": "^0.10.9",
    "bs58": "^5.0.0",
    "dotenv": "^16.3.1",
    "js-sha3": "^0.8.0",
    "mongodb": "^6.3.0",
    "pg": "^8.16.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.6",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
